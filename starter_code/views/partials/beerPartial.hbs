<div class="beer-card col-4">
    <img class="beer-img" src="{{this.image_url}}" alt="Card image cap">
    <div class="card-body">
        <h5 class="card-title text-primary">{{this.name}}</h5>
        <h6 class="card-subtitle mb-2 text-muted">{{this.tagline}}</h6>
        <p class="card-text">{{this.description}}</p>
    </div>
</div>
